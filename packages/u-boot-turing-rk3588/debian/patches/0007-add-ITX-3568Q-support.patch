+		u-boot,dm-pre-reloc;
+	};
+};
+
+&gmac1_mtl_tx_setup {
+	u-boot,dm-pre-reloc;
+	queue0 {
+		u-boot,dm-pre-reloc;
+	};
+};
+
+&mdio0 {
+	u-boot,dm-pre-reloc;
+	rgmii_phy0: phy@0 {
+		compatible = "ethernet-phy-ieee802.3-c22";
+		u-boot,dm-pre-reloc;
+		reg = <0x0>;
+	};
+};
+
+&mdio1 {
+	u-boot,dm-pre-reloc;
+	rgmii_phy1: phy@0 {
+		compatible = "ethernet-phy-ieee802.3-c22";
+		u-boot,dm-pre-reloc;
+		reg = <0x0>;
+	};
+};
+
+&gmac0_miim {
+	u-boot,dm-pre-reloc;
+};
+
+&gmac0_clkinout {
+	u-boot,dm-pre-reloc;
+};
+
+&gmac0_rx_bus2 {
+	u-boot,dm-pre-reloc;
+};
+
+&gmac0_tx_bus2 {
+	u-boot,dm-pre-reloc;
+};
+
+&gmac0_rgmii_clk {
+	u-boot,dm-pre-reloc;
+};
+
+&gmac0_rgmii_bus {
+	u-boot,dm-pre-reloc;
+};
+
+&gmac1m1_miim {
